{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "20.10.0"
  },
  "scripts": {
    "postinstall": "yarn prisma:generate",
    "start": "node ./dist/shared/infra/http/server.js",
    "dev": "tsnd --exit-child --respawn --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "prisma:studio": "prisma studio --schema=./src/shared/infra/prisma/schema.prisma ",
    "prisma:migrate": "prisma migrate dev --schema=./src/shared/infra/prisma/schema.prisma ",
    "prisma:generate": "prisma generate --schema=./src/shared/infra/prisma/schema.prisma ",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "celebrate": "^15.0.3",
    "express": "^4.18.2",
    "i": "^0.3.7",
    "npm": "^10.2.5",
    "prisma": "^5.7.1",
    "reflect-metadata": "^0.2.1",
    "ts-node-dev": "^2.0.0",
    "tsyringe": "^4.8.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/plugin-proposal-decorators": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
